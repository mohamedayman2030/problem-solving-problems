#include <bits/stdc++.h> 

using namespace std;

int main()
{ string x;
int l;
cin>>l;
while(l--){
cin>>x;

vector<char>v(x.begin(),x.end());






if( v.size()==5){
    cout<<"3"<<endl;
}
else if( (v[0]=='o' && v[1]=='n') || v[1]=='n' && v[2]=='e' || (v[0]=='o' && v[2]=='e') ){
    cout<<"1"<<endl;
}
 else if( (v[0]=='t' && v[2]=='o') || (v[0]=='t' && v[1]=='w') || (v[1]=='w' && v[2]=='o')   ){
{
    cout<<"2"<<endl;
}
}
}
return 0;
}
